{
  "name": "Patient Records and Appointment Scheduling",
  "description": "Manage patient records and scheduling of appointments",
  "tables": {
    "patients": {
      "columns": [
        {
          "name": "patient_id",
          "type": "String"
        },
        {
          "name": "name",
          "type": "String"
        },
        {
          "name": "date_of_birth",
          "type": "Date"
        }
      ],
      "key": "patient_id"
    },
    "appointments": {
      "columns": [
        {
          "name": "appointment_id",
          "type": "String"
        },
        {
          "name": "patient_id",
          "type": "String"
        },
        {
          "name": "appointment_date",
          "type": "Date"
        },
        {
          "name": "appointment_type",
          "type": "String"
        },
        {
          "name": "actual_wait_minutes",
          "type": "BigInt"
        },
        {
          "name": "duration_minutes",
          "type": "BigInt"
        }
      ],
      "key": "appointment_id"
    }
  },
  "relationships": [
    {
      "name": "patient_appointment",
      "from": "patients",
      "to": "appointments",
      "fromKey": "patient_id",
      "toKey": "patient_id"
    }
  ]
}